filename         = "kafka_input.lua"
<%
   #brokers = @brokerlist.join(':9200", "')
   brokers = @brokerlist.join(":#{@kafka_port}\", \"")
   #brokers = '"'+brokers+':9200"'
   brokers = '"'+brokers+":#{@kafka_port}\""
-%>
brokerlist       = {<%= brokers %>}
topics           = {"<%= @topics %>"}
consumer_conf    = {
    ["group.id"]          = "telemetry_collector",
    ["message.max.bytes"] = 16024,
}
topic_conf       = {}
metadata_fields  = "<%= @metadata_fields %>"
instuction_limit = 0
ticker_interval  = 60
